##
## Managed by Puppet
##

<% @configuration_data.sort.each do |parameter, value|    -%>
<%   if value.is_a?(Array)                                -%>
<%     value.each do |v|                                  -%>
<%= parameter %> <%= v %>
<%     end                                                -%>
<%   else                                                 -%>
<%= parameter %> <%= value %>
<%   end                                                  -%>
<% end                                                    -%>

# OS specific entries.
<% @os_configuration_data.sort.each do |parameter, value| -%>
<%   if value.is_a?(Array)                                -%>
<%     value.each do |v|                                  -%>
<%= parameter %> <%= v %>
<%     end                                                -%>
<%   else                                                 -%>
<%= parameter %> <%= value %>
<%   end                                                  -%>
<% end                                                    -%>
